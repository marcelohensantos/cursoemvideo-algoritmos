algoritmo "ex095"
var
   a, b: inteiro

funcao somador(a, b: inteiro) : inteiro
var

inicio
   retorne a + b
fimfuncao

inicio
   escreva("digite o primeiro numero: ")
   leia(a)

   escreva("digite o segundo numero: ")
   leia(b)

   escreval("soma: ", somador(a, b))
fimalgoritmo