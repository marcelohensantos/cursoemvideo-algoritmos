algoritmo "ex090"
var
   a, b: inteiro

procedimento somador(a, b: inteiro)
var

inicio
   escreval("soma: ", a + b)
fimprocedimento

inicio
   escreva("digite o primeiro numero: ")
   leia(a)
   
   escreva("digite o segundo numero: ")
   leia(b)
   
   somador(a, b)
fimalgoritmo